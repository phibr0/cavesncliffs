  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.6.2",
    "@types/node-fetch": "^2.5.12",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "tsc --noEmit",
    "*.{js,ts,json}": "prettier --write"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
